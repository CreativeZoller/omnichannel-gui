    "buildtest": "ng build --outputPath=..\\omnichannelcommunication-gui-deployment-development --deleteOutputPath=false --configuration=development",
    "test": "ng test --watch false",
    "e2e": "ng e2e",
    "ng": "ng",
    "generate": "node generator.js",
    "prepare": "husky install"
  },
