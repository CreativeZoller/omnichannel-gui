  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "format:fix": "pretty-quick --staged",
    "lint": "ng lint",
    "start": "ng serve",
    "startssl": "ng serve --ssl true --ssl-key c:\\certificates\\localhost.key  --ssl-cert c:\\certificates\\localhost.crt",
